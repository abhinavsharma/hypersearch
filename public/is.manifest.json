{
    "manifest_version": 2,
    "name": "Insight",
    "description": "Insight supercharges search with fast previews, trusted sources, muting and lenses that offer unique and useful perspectives.",
    "version": "0.0.86",
    "applications": {
        "gecko": {
            "id": "hi@insightbrowser.com"
        }
    },
    "web_accessible_resources": [
        "*.png",
        "*.svg",
        "*.html",
        "*.css"
    ],
    "icons": {
        "128": "logo128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "js/content_script.js"
            ],
            "run_at": "document_start"
        },
        {
            "matches": [
                "<all_urls>"
            ],
            "all_frames": true,
            "js": [
                "js/content_frames.js"
            ],
            "run_at": "document_start"
        },
        {
            "matches": [
                "<all_urls>"
            ],
            "all_frames": true,
            "js": [
                "js/content_blocker.js"
            ],
            "run_at": "document_end"
        },
        {
            "matches": [
                "<all_urls>"
            ],
            "all_frames": true,
            "js": [
                "js/reorder.js"
            ],
            "run_at": "document_end"
        },
        {
            "matches": [
                "<all_urls>"
            ],
            "all_frames": true,
            "css": [
                "overwrite.css"
            ]
        }
    ],
    "background": {
        "scripts": [
            "js/hot.js",
            "js/background.js"
        ]
    },
    "browser_action": {
        "default_title": "Insight"
    },
    "permissions": [
        "cookies",
        "storage",
        "<all_urls>",
        "webRequest",
        "webRequestBlocking",
        "webNavigation"
    ]
}
